{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": 0,
    "require-jsdoc": "off",
    "operator-linebreak": 0,
    "indent": "off",
    "space-before-function-paren": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
